mixin agents-item(img, alt, name, mobile, email)
    .agents-item
        .agents__img
            img(src="./img/agents/" + img, alt= alt)
        .agents-info
            h3.agents__info-title
                span Our 
                span Agents
            .agents__info-name #{name}
            .agents__info-desc 
                block 
            .agents__info-social
                a(href="#").agents__info-social-mobile
